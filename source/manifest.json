{
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'",
  "description": "__MSG_description__",
  "icons": {
    "128": "logo-128.png",
    "16": "logo-16.png",
    "48": "logo-48.png"
  },
  "browser_action": {
    "default_icon": "logo-48.png"
  },
  "background": {
    "scripts": ["backend.js"]
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": ["tabs", "notifications", "https://api.pixiv.moe/"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "default_locale": "en",
  "version": "0.5.9"
}
